{
  "annotations": {
    "list": []
  },
  "description": "Service Level Objectives Overview - Error Budgets and Burn Rates",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "gridPos": {"h": 3, "w": 24, "x": 0, "y": 0},
      "id": 1,
      "options": {
        "content": "# SLO Overview\nMonitor your Service Level Objectives, error budgets, and burn rates. Red indicates error budget exhaustion risk.",
        "mode": "markdown"
      },
      "type": "text"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource}"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "green", "value": 75}
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 3},
      "id": 2,
      "options": {
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "title": "Error Budget Remaining",
      "description": "Percentage of error budget remaining for the current window",
      "type": "gauge",
      "targets": [
        {
          "expr": "100 * (1 - (sum(rate(http_requests_total{status=~\"5..\", job=~\"$service\"}[30d])) / sum(rate(http_requests_total{job=~\"$service\"}[30d]))) / (1 - 0.999))",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource}"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null}
            ]
          },
          "unit": "percentunit"
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 3},
      "id": 3,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "title": "Current Availability",
      "type": "stat",
      "targets": [
        {
          "expr": "1 - (sum(rate(http_requests_total{status=~\"5..\", job=~\"$service\"}[5m])) / sum(rate(http_requests_total{job=~\"$service\"}[5m])))",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource}"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 2}
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 12, "y": 3},
      "id": 4,
      "title": "Burn Rate (1h)",
      "description": "How fast error budget is being consumed. >1 means burning faster than sustainable.",
      "type": "stat",
      "targets": [
        {
          "expr": "(sum(rate(http_requests_total{status=~\"5..\", job=~\"$service\"}[1h])) / sum(rate(http_requests_total{job=~\"$service\"}[1h]))) / (1 - 0.999)",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource}"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 2}
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 18, "y": 3},
      "id": 5,
      "title": "Burn Rate (6h)",
      "type": "stat",
      "targets": [
        {
          "expr": "(sum(rate(http_requests_total{status=~\"5..\", job=~\"$service\"}[6h])) / sum(rate(http_requests_total{job=~\"$service\"}[6h]))) / (1 - 0.999)",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource}"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "showPoints": "never",
            "spanNulls": false,
            "stacking": {"group": "A", "mode": "none"},
            "thresholdsStyle": {"mode": "line"}
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "red", "value": 0.001}
            ]
          },
          "unit": "percentunit"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 9},
      "id": 6,
      "title": "Error Rate Over Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status=~\"5..\", job=~\"$service\"}[5m])) / sum(rate(http_requests_total{job=~\"$service\"}[5m]))",
          "legendFormat": "Error Rate",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource}"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisCenteredZero": false,
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "showPoints": "never"
          },
          "unit": "s"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 9},
      "id": 7,
      "title": "Latency Percentiles",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{job=~\"$service\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=~\"$service\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=~\"$service\"}[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["slo", "sre", "availability"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 0,
        "includeAll": false,
        "label": "Datasource",
        "multi": false,
        "name": "datasource",
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      },
      {
        "datasource": {"type": "prometheus", "uid": "${datasource}"},
        "definition": "label_values(http_requests_total, job)",
        "hide": 0,
        "includeAll": true,
        "label": "Service",
        "multi": true,
        "name": "service",
        "query": "label_values(http_requests_total, job)",
        "refresh": 2,
        "type": "query"
      }
    ]
  },
  "time": {"from": "now-24h", "to": "now"},
  "title": "SLO Overview",
  "uid": "slo-overview",
  "version": 1
}
